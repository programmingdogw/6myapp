<% provide(:title, @user.name) %>



<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <%= gravatar_for @user %>
        <%= @user.name %>
      </h1>
    </section>
  </aside>
</div>

<% @pictures.each do |picture| %>
  <%= image_tag picture.image, :size =>'120x180' %>
<% end %>

<%=@user%><br>
<%=@user.name%><br>
<%=@user.birthdate%><br>
<%=@user.deathdate%><br>