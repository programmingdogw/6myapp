<%= form_for(@picture, url: {controller: 'pictures', action: 'create' }) do |f| %>
  <%= f.hidden_field :user_id, :value => current_user.id %>
  <%= f.label :image %>
  <%= f.file_field :image %>
  <%= f.submit %>
<% end %>


<%= form_with(model: @picture, url: createimage_path, method: :post, local: true) do |f| %>
 
  <%= f.file_field :image %>
  <%= f.submit '画像をアップロードする' %>
<% end %>

