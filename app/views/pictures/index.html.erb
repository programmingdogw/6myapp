<%= form_for(@picture, url: {controller: 'pictures', action: 'create' }) do |f| %>
  <%= f.hidden_field :user_id, :value => current_user.id %>
  <%= f.label :image %>
  <%= f.file_field :image %>
  <%= f.submit %>
<% end %>
<% @pictures.each do |picture| %>
                
                    <%= image_tag picture.image, :size =>'120x180' %>
                
            <% end %>
 